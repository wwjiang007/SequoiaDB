<optOtherInfoForWeb>
   <title>
      <en>Database Runtime Configurtion</en>
      <cn>数据库配置</cn>
   </title>
   <subtitle>
      <en>Parameter Instruction</en>
      <cn>参数说明</cn>
   </subtitle>
   <sthead>
      <stentry_name>
         <en>Parameter Name</en>
         <cn>参数名</cn>
      </stentry_name>
      <stentry_acronym>
         <en>Acronym</en>
         <cn>缩写</cn>
      </stentry_acronym>
