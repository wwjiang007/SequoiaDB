<div id="OperateRecord" style="padding:10px 15px;" ng-controller="Data.Operate.Record.Ctrl">
   <div class="underlineTab" data-height="50">
      <ul class="right">
         <li class="text Ellipsis" style="width:350px;">
            <b>{{fullName}}</b>
         </li>
         <li ng-if="clType != 'main'">
            <a href="#/Data/Operate/Lobs">{{autoLanguage("大对象操作")}}</a>
         </li>
         <li class="active">
            <a href="#/Data/Operate/Record">{{autoLanguage("记录操作")}}</a>
         </li>
      </ul>
   </div>
   <div id="btnGroup" data-height="30">
      <button class="topBtn" ng-click="queryAll()"><i class="fa fa-search"></i> {{autoLanguage("所有")}}</button>
      <button class="topBtn" ng-click="Query()"><i class="fa fa-search"></i> {{autoLanguage("查询")}}</button>
      <button class="topBtn" ng-click="Insert()"><i class="fa fa-plus"></i> {{autoLanguage("插入")}}</button>
      <button class="topBtn" ng-click="Update()"><i class="fa fa-edit"></i> {{autoLanguage("更新")}}</button>
      <button class="topBtn" ng-click="Delete()"><i class="fa fa-remove"></i> {{autoLanguage("删除")}}</button>
   </div>
   <div data-mtop="12" data-height="40" class="result Ellipsis" ng-class="{true:'success',false:'failed'}[execRc]">
      {{execResult}}
   </div>
   <div id="GridParent" style="position:relative;" data-mtop="13" data-offsety="-145">
      <div ng-show="GridData['body'].length > 0" create-grid para="GridData"></div>
      <div ng-show="GridData['body'].length == 0">
         <div class="data_empty">
            <div class="title">{{autoLanguage("集合没有数据")}}</div>
            <div class="tips">{{autoLanguage("使用\"插入\"操作添加数据。")}}</div>
         </div>
      </div>
   </div>
</div>