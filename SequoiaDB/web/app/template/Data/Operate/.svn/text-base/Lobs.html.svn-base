<div id="LobIndex" style="padding:10px 15px; min-width:710px;" ng-controller="Data.Lob.Lobs.Ctrl">
   <div class="underlineTab" data-height="50">
      <ul class="right">
         <li class="text Ellipsis" style="width:350px;">
            <b>{{fullName}}</b>
         </li>
         <li class="active">
            <a href="#/Data/Operate/Lobs">{{autoLanguage("大对象操作")}}</a>
         </li>
         <li>
            <a href="#/Data/Operate/Record">{{autoLanguage("记录操作")}}</a>
         </li>
      </ul>
   </div>
   <div id="btnGroup" data-height="30">
      <button class="topBtn" ng-click="queryAll()"><i class="fa fa-search"></i> {{autoLanguage("所有")}}</button>
      <button class="topBtn" ng-click="LobQuery()"><i class="fa fa-search"></i> {{autoLanguage("大对象查询")}}</button>
   </div>
   <div data-mtop="12" data-height="40" class="result Ellipsis" ng-class="{true:'success',false:'failed'}[execRc]">
      {{execResult}}
   </div>
   <div id="GridParent" style="position:relative;" data-mtop="13" data-offsety="-145">
      <div ng-show="lobGridData['body'].length > 0" create-grid para="lobGridData"></div>
      <div ng-show="lobGridData['body'].length == 0">
         <div class="data_empty">
            <div class="title">{{autoLanguage("集合没有数据")}}</div>
            <div class="tips">{{autoLanguage("使用工具可以导入Lob。")}}</div>
         </div>
      </div>
   </div>
</div>