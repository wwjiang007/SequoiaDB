<div style="padding:10px 15px;" ng-controller="Data.Operate.Record.Ctrl" ng-container="{}">
   <div class="underlineTab" ng-container="{height:50}">
      <ul class="right">
         <li class="text Ellipsis" style="width:350px;">
            <b><a class="linkButton" href="#/Data/SDB-Database/Index">{{autoLanguage("数据库")}}</a> <span style="font-size:80%; color:#666;">></span> <span style="color:#555;">{{fullName}}</span></b>
           
         </li>
         <li ng-if="clType != 'main'">
            <a href="#/Data/SDB-Operate/Lobs">{{autoLanguage("大对象操作")}}</a>
         </li>
         <li class="active">
            <a href="#/Data/SDB-Operate/Record">{{autoLanguage("记录操作")}}</a>
         </li>
      </ul>
   </div>
   <div ng-container="{height:30}">
      <button class="topBtn" ng-click="queryAll()"><i class="fa fa-search"></i> {{autoLanguage("所有")}}</button>
      <button class="topBtn" ng-click="Query()"><i class="fa fa-search"></i> {{autoLanguage("查询")}}</button>
      <button class="topBtn" ng-click="Insert()"><i class="fa fa-plus"></i> {{autoLanguage("插入")}}</button>
      <button class="topBtn" ng-click="Update()"><i class="fa fa-edit"></i> {{autoLanguage("更新")}}</button>
      <button class="topBtn" ng-click="Delete()"><i class="fa fa-remove"></i> {{autoLanguage("删除")}}</button>
   </div>
   <div class="result Ellipsis" ng-class="{true:'success',false:'failed'}[execRc]" ng-container="{height:40,marginTop:12}">
      {{execResult}}
   </div>
   <div style="position:relative;" ng-container="{marginTop:13,offsetY:-145}">
      <div ng-show="GridData['body'].length > 0" create-grid="GridData" ng-container="{}"></div>
      <div ng-show="GridData['body'].length == 0" ng-container="{}">
         <div class="data_empty">
            <div class="title">{{autoLanguage("集合没有数据")}}</div>
            <div class="tips">{{autoLanguage("使用\"插入\"操作添加数据。")}}</div>
         </div>
      </div>
   </div>
</div>