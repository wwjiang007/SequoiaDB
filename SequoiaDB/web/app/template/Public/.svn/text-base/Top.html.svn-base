<div ng-controller="Index.Top.Ctrl">
   <div id="PublicLogo" class="pull-left">
      <img class="PublicLogoImg" src="./images/logo-1.png">
      <span class="PublicLogo1"  ng-click="loading()">Sequoia</span><span class="PublicLogo2">DB</span>&nbsp;&nbsp;<span class="PublicLogo3">SAC</span>
      <div class="clear-float"></div>
   </div>
   <div id="PublicNav1" class="pull-right">
      <ul>
         <li>
            <div ng-click="Top.showUserMenu()"><i class="fa fa-user"></i>&nbsp;<span>admin</span>&nbsp;<i class="fa fa-caret-down"></i></div>
            <div id="userMenu">
               <ul>
                  <li ng-click="showChangePasswd()"><i class="fa fa-key"></i> {{autoLanguage('修改密码')}}</li>
                  <li ng-click="logout()"><i class="fa fa-sign-out"></i> {{autoLanguage('注销')}}</li>
               </ul>
            </div>
         </li>
      </ul>
   </div>
   <div class="clear-float"></div>
</div>
