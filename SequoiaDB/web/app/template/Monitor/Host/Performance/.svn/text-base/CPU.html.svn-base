<div id="PerformanceCPU" style="padding:10px 15px;" ng-controller="Performance.CPU.Index.Ctrl" ng-container="{}">
   <div class="underlineTab" ng-container="{height:50}">
      <ul class="right">
         <li class="ng-scope" style="float:left;">
            <a href="#/Monitor/HostPerformance/Index" class="ng-binding">系统总览</a>
         </li>
         <li class="ng-scope" style="float:left;">
            <a href="#/Monitor/HostPerformance/Disk" class="ng-binding">磁盘</a>
         </li>
         <li class="ng-scope" style="float:left;">
            <a href="#/Monitor/HostPerformance/Memory" class="ng-binding">内存</a>
         </li>
         <li class="active" style="float:left;">
            <a href="#/Monitor/HostPerformance/CPU" class="ng-binding">CPU</a>
         </li>
         <li class="ng-scope" style="float:left;">
            <a href="#/Monitor/HostPerformance/Network" class="ng-binding">网卡</a>
         </li>
      </ul>
   </div>
   <div ng-container="{height:40,marginBottom:10}">
      <div class="result Ellipsis ng-binding success" style="height:38px;">
         2016-01-19 15:59  CPU处于健康运作状态
      </div>
   </div>
   <div class="content" style="overflow-y:auto;" ng-container="{offsetY:-100}">
      <div style="overflow-y:auto; padding-top:12px; border:1px solid #ddd;" ng-container="{height:270,marginBottom:15,offsetX:-20}">
         <div ng-container="{height:19,width:550,marginBottom:15,marginLeft:20}">
            <span style="color:#00BFDD; font-size:16px; font-weight:bold; ">CPU 1 - Intel(R) Xeon(R) CPU E5-2640 v2</span>
         </div>
         <div style="float:left; font-size:90%;" ng-container="{width:500,height:220,marginLeft:20}">
            <div>
               <table>
                  <tbody>
                     <tr>
                        <td style="width:130px;">Model_name</td>
                        <td style="color:#666; font-size:12px; font-weight:bold;">Intel(R) Xeon(R) CPU E5-2640 v2 @ 2.00GHz</td>
                     </tr>
                     <tr>
                        <td>Cores</td>
                        <td style="color:#666; font-size:12px; font-weight:bold;">4</td>
                     </tr>
                     <tr>
                        <td>Mhz</td>
                        <td style="color:#666; font-size:12px; font-weight:bold;">2000.00</td>
                     </tr>
                     <tr>
                        <td>Cache Size</td>
                        <td style="color:#666; font-size:12px; font-weight:bold;">2048kb</td>
                     </tr>
                     <tr>
                        <td>Model</td>
                        <td style="color:#666; font-size:12px; font-weight:bold;">64</td>
                     </tr>
                  </tbody>
               </table>
            </div>
         </div>
         <div style="float:left;" ng-container="{offsetX:-550,height:220}">
            <div style="height:220px; float:left;" ng-container="{width:620}">
               <div style="color:#666; font-size:12px; font-weight:bold; padding-left:20px;"></div>
               <div style="width:95%; height:220px; float:left;" para="charts.Cpu" create-line-chart></div>
            </div>
         </div>
      </div>
      <div style="overflow-y:auto; padding-top:12px; border:1px solid #ddd;" ng-container="{height:270,marginBottom:15,offsetX:-20}">
         <div ng-container="{height:19,width:550,marginBottom:15,marginLeft:20}">
            <span style="color:#00BFDD; font-size:16px; font-weight:bold; ">CPU 2 - Intel(R) Xeon(R) CPU E5-2640 v2</span>
         </div>
         <div style="float:left; font-size:90%;" ng-container="{width:500,height:220,marginLeft:20}">
